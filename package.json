{
    "name": "devstash",
    "displayName": "DevStash",
    "description": "Quickly save code snippets, commands, and notes to your personal knowledge vault.",
    "publisher": "bugcacher",
    "version": "0.1.1",
    "icon": "assets/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/bugcacher/devstash-vscode-extension.git"
    },
    "bugs": {
        "url": "https://github.com/bugcacher/devstash-vscode-extension/issues"
    },
    "homepage": "https://github.com/bugcacher/devstash-vscode-extension",
    "keywords": [
        "snippets",
        "code",
        "search",
        "vault",
        "stash"
    ],
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "devstash.saveSelection",
                "title": "Devstash: Save"
            },
            {
                "command": "devstash.search",
                "title": "Devstash: Search"
            }
        ],
        "keybindings": [
            {
                "command": "devstash.saveSelection",
                "key": "ctrl+alt+s",
                "mac": "cmd+alt+s",
                "when": "editorHasSelection"
            },
            {
                "command": "devstash.search",
                "key": "ctrl+alt+d",
                "mac": "cmd+alt+d"
            }
        ],
        "configuration": {
            "title": "DevStash",
            "properties": {
                "devstash.webhookUrl": {
                    "type": "string",
                    "default": "",
                    "description": "The webhook URL to send your stashed code snippets to."
                },
                "devstash.authToken": {
                    "type": "string",
                    "default": "",
                    "description": "Optional bearer token for authenticating with your webhook endpoint."
                },
                "devstash.algoliaAppId": {
                    "type": "string",
                    "default": "",
                    "description": "Your Algolia Application ID."
                },
                "devstash.algoliaApiKey": {
                    "type": "string",
                    "default": "",
                    "description": "Your Algolia Search-Only API Key."
                },
                "devstash.algoliaIndexName": {
                    "type": "string",
                    "default": "",
                    "description": "The name of your Algolia index."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts"
    },
    "devDependencies": {
        "@types/vscode": "^1.80.0",
        "@types/node": "20.x",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.11.0",
        "@typescript-eslint/parser": "^7.11.0",
        "eslint": "^8.57.0",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "axios": "^1.7.2",
        "uuid": "^9.0.1",
        "algoliasearch": "^4.20.0"
    }
}